[defaults]
host_key_checking=False

[nodes]
192.168.199.3 ansible_user=ubuntu
192.168.200.3 ansible_user=ubuntu
192.168.201.3 ansible_user=ubuntu

[nodes:vars]
ansible_ssh_common_args='-i {{ ssh_key_path }} -o ProxyCommand="ssh -i {{ ssh_key_path }} -o StrictHostKeyChecking=no -W %h:%p -q ubuntu@{{ manage_ip }}"'
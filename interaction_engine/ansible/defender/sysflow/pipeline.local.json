{
    "pipeline":[
        {
        "processor": "sysflowreader",
        "handler": "flattener",
        "in": "sysflow sysflowchan",
        "out": "flat flattenerchan"
        },
        {
        "processor": "policyengine",
        "in": "flat flattenerchan",
        "out": "evt eventchan",
        "policies": "/etc/sysflow/policies/distribution/filter.yaml",
        "mode": "alert"
        },
        {
        "processor": "exporter",
        "in": "evt eventchan",
        "export": "file",
        "format": "json"
        }
    ]
}
  